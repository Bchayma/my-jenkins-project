pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the project...'
                bat 'echo Build command here'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
                bat 'echo Test command here'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the application...'
                bat 'echo Deploy command here'
            }
        }
    }
}
